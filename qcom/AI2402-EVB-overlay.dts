};
&tlmm{
	sx932x_active {
		sx932x_active: sx932x_active {
			mux {
				pins = "gpio177";
				function = "gpio";
			};
			config {
				pins = "gpio177";
				bias-pull-up;
				drive-strength = <6>;
			};
		};
	};
	sx932x_suspend {
		sx932x_suspend: sx932x_suspend {
			mux {
				pins = "gpio177";
				function = "gpio";
			};
			config {
				pins = "gpio177";
				bias-disable;
				drive-strength = <2>;
			};
		};
	};
};
